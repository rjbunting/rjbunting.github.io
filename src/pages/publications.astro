  </HorizontalCard>

  <HorizontalCard
    title="Reactivity of Single-Atom Alloy Nanoparticles: Modeling the Dehydrogenation of Propane"
    img="/reactivity.gif"
    authors="RJ Bunting*, F Wodaczek, T Torabi, B Cheng"
    desc="Journal of the American Chemical Society"
    url="https://doi.org/10.1021/jacs.3c04030"
  />

  <HorizontalCard
    title="Understanding and tackling the activity and selectivity issues for methane to methanol using single atom alloys"
    img="/understanding.gif"
    authors="RJ Bunting, PS Rice, Z Yao, J Thompson, P Hu*"
    desc="Chemical Communications"
    url="https://doi.org/10.1039/D2CC03183C"
  />

  <HorizontalCard
      title=""
      img="/quantitative.gif"
      authors="Z Yao, J Zhao, RJ Bunting, C Zhao, P Hu, J Wang*"
